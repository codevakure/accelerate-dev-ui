  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --output-hashing=all --base-href ./",
    "production": "ng build --configuration=production --output-hashing=all --base-href ./",
    "test-dev": "ng build --configuration=test-dev --output-hashing=all --base-href ./",
